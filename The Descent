while (true) {
    var maxh = 0;
    var t = null;
    for (var i = 0; i < 8; i++) {
        var mountainH = parseInt(readline()); // represents the height of one mountain, from 9 to 0.
        if (mountainH > maxh){
            maxh = mountainH;
            t = i;
        }
    }

    print(t); 
}
